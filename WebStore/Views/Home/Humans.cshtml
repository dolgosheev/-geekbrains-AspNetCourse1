@model IEnumerable<WebStore.Models.Human>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Члены семьи</title>
</head>
<body>

    <h3>Члены семьи</h3>
<hr />

<table>
    <thead>
    <tr>
        <th>#</th>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Отчество</th>
        <th>Возраст</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var member in Model)
    {
        <tr>
            <td>@member.Id</td>
            <td>@member.LastName</td>
            <td>@member.FirstName</td>
            <td>@member.Patronymic</td>
            <td>@member.Age</td>
        </tr>
    }
    </tbody>
</table>

<p>
    <a href="/tipofaday">Совет дня</a> | <a href="/">Домой</a>
</p>

</body>
</html>